[run]
source = .
omit = 
    test_*.py
    verify_android_python_match.py
    */site-packages/*
    */__pycache__/*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:

[html]
directory = htmlcov

[xml]
output = coverage.xml
